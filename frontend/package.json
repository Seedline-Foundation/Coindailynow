    "@types/bcryptjs": "^3.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "elliptic": "^6.6.1",
    "cookie": "^0.7.0"
  }
}
