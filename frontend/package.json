{
  "name": "coindaily-frontend",
  "version": "1.0.0",
  "description": "CoinDaily Platform Frontend - Africa's Premier Cryptocurrency News Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:critical-css": "npm run build && node scripts/critical-css.js",
    "build:production": "npm run type-check && npm run lint && npm run test && npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:unit:coverage": "jest --testPathPattern=tests/unit --coverage",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:security": "jest --testPathPattern=tests/unit/(security-dashboard|audit-system|accessibility|rate-limiting)",
    "test:all": "jest --coverage && playwright test",
    "test:responsive": "ts-node tests/responsive.test.ts",
    "test:accessibility": "ts-node tests/accessibility.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:security": "playwright test tests/integration/security-e2e.test.ts",
    "test:e2e:critical": "playwright test tests/e2e/critical-paths.spec.ts",
    "test:browsers": "playwright test --project=chromium --project=firefox --project=webkit",
    "analyze-bundle": "ts-node scripts/analyze-bundle.ts",
    "perf:bundle": "npm run build && npm run analyze-bundle",
    "perf:lighthouse": "lhci autorun",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --collect.url=http://localhost:3000 --collect.numberOfRuns=3",
    "perf": "npm run build && npm run lighthouse",
    "analyze": "ANALYZE=true npm run build",
    "validate:production": "npm run type-check && npm run lint && npm run test:all && npm run test:responsive && npm run test:accessibility && npm run lighthouse",
    "checklist": "ts-node production-checklist.ts",
    "checklist:critical": "ts-node production-checklist.ts --critical-only"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@next/bundle-analyzer": "^14.0.4",
    "@prisma/client": "^6.16.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "@walletconnect/core": "^2.21.9",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/qrcode-modal": "^1.8.0",
    "@walletconnect/sign-client": "^2.21.9",
    "@walletconnect/types": "^2.21.9",
    "@walletconnect/utils": "^2.21.9",
    "@walletconnect/web3wallet": "^1.16.1",
    "autoprefixer": "^10.4.16",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "beasties": "^0.3.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critical": "^5.2.0",
    "date-fns": "^3.0.6",
    "ethers": "^5.8.0",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.2",
    "ioredis": "^5.8.1",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "^14.0.4",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.4.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.2.1",
    "sharp": "^0.33.5",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.6",
    "viem": "^2.37.8",
    "web-vitals": "^3.5.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
