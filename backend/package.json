{
  "name": "coindaily-backend",
  "version": "1.0.0",
  "description": "CoinDaily Platform Backend - Africa's Premier Cryptocurrency News Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=tests/api",
    "test:api:coverage": "jest --testPathPattern=tests/api --coverage",
    "test:security": "jest --testPathPattern=tests/api/security",
    "test:e2e": "jest --testPathPattern=tests/integration/ai-system/e2e",
    "test:integration": "jest --testPathPattern=tests/integration/ai-system/api-integration",
    "test:performance": "jest --testPathPattern=tests/integration/ai-system/performance",
    "test:ai": "jest --testPathPattern=tests/integration/ai-system",
    "test:load": "artillery run tests/load/artillery-config.yml",
    "test:stress": "artillery run tests/load/stress-test.yml",
    "test:load:report": "artillery run tests/load/artillery-config.yml -o report.json && artillery report report.json -o report.html",
    "test:all": "jest --coverage",
    "test:ci": "npm run test:coverage && npm run test:load",
    "perf:benchmark": "ts-node scripts/benchmark-api.ts",
    "perf:db-optimize": "ts-node scripts/optimize-database.ts",
    "perf:all": "npm run perf:benchmark && npm run perf:db-optimize",
    "security:test": "ts-node scripts/security-penetration-test.ts",
    "security:audit": "npm audit --json > security-audit.json && cat security-audit.json",
    "db:types": "npx supabase gen types typescript --project-id auakxtwvqqefysprkczv > types/database.types.ts",
    "migration:check": "node scripts/migrate-to-supabase.js",
    "migration:status": "pwsh ../check-migration-status.ps1",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc --noEmit"
  },
  "keywords": [
    "cryptocurrency",
    "news",
    "africa",
    "ai",
    "graphql",
    "typescript"
  ],
  "author": "CoinDaily Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@elastic/elasticsearch": "^8.10.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google-cloud/vertexai": "^1.10.0",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@prisma/client": "^6.17.0",
    "@supabase/supabase-js": "^2.91.1",
    "@types/axios": "^0.14.4",
    "@types/helmet": "^4.0.0",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.2",
    "@types/sharp": "^0.32.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "fast-xml-parser": "^5.3.0",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.2",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.1",
    "isomorphic-dompurify": "^2.28.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "openai": "^5.23.2",
    "otplib": "^12.0.1",
    "pg": "^8.17.1",
    "redis": "^5.8.3",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
